{
  "name": "@nitish-raj/searxng-mcp-bridge",
  "version": "0.10.11",
  "description": "MCP Server for SearxNG Bridge",
  "private": false,
  "type": "module",
  "bin": {
    "mcp-searxng-bridge": "./build/index.js"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json && node -e \"require('fs').chmodSync('build/index.js', 0o755)\"",
    "prepare": "npm run build",
    "watch": "tsc --watch -p tsconfig.json",
    "start": "node build/index.js",
    "start:http": "TRANSPORT=http node build/index.js",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "release:major": "node scripts/release.js major",
    "release:minor": "node scripts/release.js minor",
    "release:patch": "node scripts/release.js patch",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.24.3",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "author": "nitish-raj",
  "license": "MIT",
  "keywords": [
    "mcp",
    "mcp-server",
    "searxng",
    "search",
    "bridge"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nitish-raj/searxng-mcp-bridge.git"
  },
  "bugs": {
    "url": "https://github.com/nitish-raj/searxng-mcp-bridge/issues"
  },
  "homepage": "https://github.com/nitish-raj/searxng-mcp-bridge#readme",
  "engines": {
    "node": ">=20.0.0"
  }
}
